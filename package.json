{
  "name": "natours",
  "version": "1.0.0",
  "description": "project to learn node and express",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coderwurst/natours-project.git"
  },
  "author": "coderwurst (andrew mcmechan)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coderwurst/natours-project/issues"
  },
  "homepage": "https://github.com/coderwurst/natours-project#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.1",
    "pug": "^2.0.4",
    "sharp": "^0.23.4",
    "slugify": "^1.3.5",
    "validator": "^11.1.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": "8.3.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "3",
    "verbose": true,
    "autoAttachChildProcesses": true,
    "execMap": {
      "js": "node --harmony"
    },
    "ext": "js,json"
  }
}
